(rule
 (targets test.out.atd)
 (deps test.atd)
 (action (run %{bin:atdcat} %{deps} -o %{targets})))

(rule
 (alias runtest)
 (deps test.out.atd)
 (action (diff test.expected.atd test.out.atd)))

(rule
 (targets test2.out.atd)
 (deps test2.atd)
 (action (run %{bin:atdcat} %{deps} -o %{targets})))

(rule
 (alias runtest)
 (deps test2.out.atd)
 (action (diff test2.expected.atd test2.out.atd)))
